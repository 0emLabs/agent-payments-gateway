{
  "name": "@0emlabs/agent-payments-gateway",
  "version": "0.1.0",
  "description": "The payment infrastructure for the AI agent economy.",
  "scripts": {
    "dev": "bun run --filter @0emlabs/agent-payments-worker dev",
    "build": "bun run --filter @0emlabs/agent-payments-worker build",
    "test": "bun run --filter @0emlabs/agent-payments-worker test",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,toml,sql,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,toml,sql,yml}\""
  },
  "keywords": [
    "cloudflare",
    "workers",
    "hono",
    "typescript",
    "ai",
    "agents",
    "payments",
    "web3",
    "x402",
    "micropayments",
    "stablecoin",
    "base"
  ],
  "author": "0EM Labs",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240403.0",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "bun-types": "latest",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-sql": "^0.19.2",
    "prettier-plugin-toml": "^0.3.5",
    "typescript": "^5.4.5"
  },
  "trustedDependencies": [
    "@esbuild/darwin-arm64"
  ]
}
